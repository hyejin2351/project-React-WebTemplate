{
  "name": "was-starter",
  "version": "0.1.0",
  "description": "Web Application Server Starter",
  "engines": {
    "node": ">=6.9.4"
  },
  "scripts": {
    "win-dev": "set DEBUG=app:*,-app:HNDataAPI & npm run nodemon",
    "dev": "DEBUG=app:*,-app:HNDataAPI npm run nodemon",
    "nodemon": "nodemon src/server/index.js --exec node",
    "debug-all": "DEBUG=* npm run nodemon",
    "debug-mode": "node --inspect src/server/index.js",
    "graphiql": "node ./src/server/graphiql/server.js",
    "mongod": "sudo systemctl start mongod",
    "test": "jest",
    "upp": "yarn upgrade file:./src/custom_modules/passport-local-mongoose",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "npm run lint -- --fix",
    "postinstall": "yarn build-next",
    "build-clean": "rm -rf build/app",
    "build-res": "cp -R src/client/static build/app/client/static && cp -R public build/",
    "build-next": "next build src/client",
    "build-server": "cp -R src/server build/app/",
    "build": "NODE_ENV=production npm run build-clean && npm run build-next && npm run build-res && npm run build-server",
    "build-export": "next export src/client -o build/app-static",
    "build-static-website": "NODE_ENV=production rm -rf build/client/static && npm run build-next && npm run build-export",
    "start": "NODE_ENV=production node build/app/server/index.js",
    "createData": "node task/createData.js"
  },
  "author": "peterk",
  "license": "Apache-2.0",
  "keywords": [
    "nextjs",
    "graphql",
    "apollo",
    "react"
  ],
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.1.0",
    "apollo-cache-inmemory": "^1.1.10",
    "apollo-client": "^2.2.6",
    "apollo-fetch": "^0.7.0",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.3",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.17.2",
    "connect-mongo": "^2.0.1",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "firebase": "^4.3.1",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "graphql-upload": "^8.0.0",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^8.2.1",
    "jss": "^9.8.1",
    "lru-cache": "^4.1.1",
    "mongoose": "^5.1.2",
    "morgan": "^1.9.0",
    "next": "^6.0.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth2": "^0.1.6",
    "passport-kakao": "0.0.5",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "file:./src/custom_modules/passport-local-mongoose",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-jss": "^8.4.0",
    "react-render-html": "^0.6.0",
    "styled-jsx": "^2.2.6",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "jest": "^23.1.0",
    "jest-enzyme": "^6.0.0",
    "mockdate": "^2.0.2",
    "nodemon": "^1.12.0",
    "react-test-renderer": "^16.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/client/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/tmp/"
    ]
  },
  "nodemonConfig": {
    "delay": "500",
    "verbose": false,
    "watch": [
      "src/server"
    ]
  }
}
